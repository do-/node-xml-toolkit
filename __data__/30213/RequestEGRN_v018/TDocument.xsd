                                <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStart" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия C</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStop" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия ПО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="notaryInfo" type="tns:TNotaryInfo">
                            <xs:annotation>
                                <xs:documentation>Реквизиты о нотариальном заверении документа
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="officialPublicationSource" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Источник официальной публикации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TConfirmPrivilege">
        <xs:complexContent>
            <xs:restriction base="tns:TStrictDocument">
                <xs:sequence>
                    <xs:sequence>
                        <xs:element maxOccurs="2" name="documentTypes" type="tns:DocumentTypes">
                            <xs:annotation>
                                <xs:documentation>Информация по кодам документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="name" type="tns:SDocName">
                            <xs:annotation>
                                <xs:documentation>Наименование документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="number" type="tns:SDocNumber">
                            <xs:annotation>
                                <xs:documentation>Номер документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issueDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата выдачи</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="attachment" type="tns:TAttachment">
                            <xs:annotation>
                                <xs:documentation>Описание приложенного файла</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="notes"
                            type="commons:TNote">
                            <xs:annotation>
                                <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="series" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Серия документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issuer" type="tns:TIssuerInfo">
                            <xs:annotation>
                                <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStart" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия C</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStop" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия ПО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="notaryInfo" type="tns:TNotaryInfo">
                            <xs:annotation>
                                <xs:documentation>Реквизиты о нотариальном заверении документа
                            </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TFileDesc">
        <xs:annotation>
            <xs:documentation>Описание файла с подписью</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" name="file" type="tns:TAttachmentFile">
                <xs:annotation>
                    <xs:documentation>Атрибуты файла прикладываемого документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" name="signature" type="tns:TAttachmentFile"
                minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Атрибуты файла электронной подписи прикладываемого документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TNotaryInfo">
        <xs:annotation>
            <xs:documentation>Реквизиты о нотариальном заверении документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="register" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Кто зарегистрировал (ФИО нотариуса)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registryNumber" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер нотариального документа в реестре нотариальных действий</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateOfCertification" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TIssuerInfo">
        <xs:annotation>
            <xs:documentation>Сведения о организации, выдавшей документ, или авторе документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код подразделения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TAttachment">
        <xs:annotation>
            <xs:documentation>Прикладываемый документ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="desc" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Описание прилагаемого документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="fileDesc" type="tns:TFileDesc">
                <xs:annotation>
                    <xs:documentation>Атрибуты файла</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="receivedInPaper" type="tns:TReceivedInPaper">
                <xs:annotation>
                    <xs:documentation>Сведения о количестве экземпляров и листов </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TAttachmentFile">
        <xs:annotation>
            <xs:documentation>Описание файла документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="1" name="fileURI" type="xs:string">
                <xs:annotation>
                    <xs:documentation>URI файла</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="md5sum" type="xs:string">
                <xs:annotation>
                    <xs:documentation>MD5 сумма для прикладываемого файла</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="fileSize" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Размер файла в килобайтах</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="fileCreationDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата создания файла</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TQuantity">
        <xs:annotation>
            <xs:documentation>Количество листов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="pageCount" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Количество листов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="docCount" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Количество экземпляров</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TReceivedInPaper">
        <xs:annotation>
            <xs:documentation>Бумажный документ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="original" type="tns:TQuantity" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Оригинал</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="copy" type="tns:TQuantity" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Копия</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="idPayersType">
        <xs:annotation>
            <xs:documentation>Идентификаторы плательщика</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="idPayer" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Идентификатор плательщика</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="(\s*[^\s]\s*)+"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="SSNILS">
        <xs:annotation>
            <xs:documentation>Страховой номер индивидуального лицевого счёта (СНИЛС)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{3}\-?\d{3}\-?\d{3}\s?\d{2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SOrgOGRN">
        <xs:annotation>
            <xs:documentation>Основной государствкенный регистрационный номер (ОГРН) организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="13"/>
            <xs:pattern value="\d+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SLegalPersonOGRN">
        <xs:annotation>
            <xs:documentation>Основной государствкенный регистрационный номер (ОГРН) индивидуального предпринимателя</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="15"/>
            <xs:pattern value="\d+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SOrgINN">
        <xs:annotation>
            <xs:documentation>Идентификационный номер налогоплательщика (ИНН) организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="10"/>
            <xs:pattern value="\d+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SPersonINN">
        <xs:annotation>
            <xs:documentation>Идентификационный номер налогоплательщика (ИНН) физического лица</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="12"/>
            <xs:pattern value="\d+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SOrgKPP">
        <xs:annotation>
            <xs:documentation>Код причины постановки на учет</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}[\w\d]{2}\d{3}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SOrgKIO">
        <xs:annotation>
            <xs:documentation>Код иностранной организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{5}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocSeries">
        <xs:annotation>
            <xs:documentation>Серия документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocNumber">
        <xs:annotation>
            <xs:documentation>Номер документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="250"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocName">
        <xs:annotation>
            <xs:documentation>Наименование документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocTypeCode">
        <xs:annotation>
            <xs:documentation>Мнемоника типа документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token"/>
    </xs:simpleType>
    <xs:simpleType name="SBIC">
        <xs:annotation>
            <xs:documentation>строка БИК, состоящая из 9 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="9"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SSettlementAccount">
        <xs:annotation>
            <xs:documentation>Расчётный счёт</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DRepresentativeDocument">
        <xs:annotation>
            <xs:documentation>Документы подтверждающие полномочия представителя</xs:documentation>
        </xs:annotation>
        <xs:restriction base="dDocument:DDocument">
            <xs:pattern value="558301\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="TOffline">
        <xs:sequence>
            <xs:element name="offlineUIN" type="xs:boolean" minOccurs="1" fixed="true">
                <xs:annotation>
                    <xs:documentation>Признак необходимости импортировать УИН в ГИС ГМП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="altPayerIdentifier" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Идентификатор плательщика ФЛ в ГИС ГМП</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unifiedPayerIdentifier" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Идентификатор плательщика ЮЛ или ИП в ГИС ГМП</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
