<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    xmlns:tns="http://rosreestr.ru/services/v0.1/commons/Documents"
    xmlns:commons="http://rosreestr.ru/services/v0.1/commons/Commons"
    xmlns:dDocument="http://rosreestr.ru/services/v0.1/commons/directories/document"
    xmlns:dReD="http://rosreestr.ru/services/v0.1/commons/directories/requestDocument"
    targetNamespace="http://rosreestr.ru/services/v0.1/commons/Documents">
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/Commons"
        schemaLocation="Commons.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/directories/document"
        schemaLocation="Dictionary/DDocument.xsd"/>

    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/directories/requestDocument"
        schemaLocation="Dictionary/DRequestDocument.xsd"/>
    <xs:complexType name="TDocumentRefer">
        <xs:annotation>
            <xs:documentation>Ссылка на документ по его идентификатору</xs:documentation>
        </xs:annotation>
        <xs:attribute name="documentID" use="required">
            <xs:annotation>
                <xs:documentation>Идентификатор документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TBasicSomeDocument">
        <xs:annotation>
            <xs:documentation>Базовый класс для групп документов в header у МИВ и ЕГРП</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="document" type="tns:TDocument" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="otherDocument" type="tns:TOtherDocumentRestr">
                <xs:annotation>
                    <xs:documentation>Прочий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentDocument" type="tns:TPaymentDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Платежный документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idDocument" type="tns:TIdDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorneyRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mapPlanDocument" type="tns:TMapPlanDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Тех. план/межевой план/акт обследования/карта-план</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="interdepDocument" type="tns:TInterdepDocument" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ сведений по МИВ (Межведомственному Информационному Взаимодействию):
                        - карта-план
                        - описание местоположения
                        - сведения для внесения в реестр ОН
                        - сведения для внесения в реестр Прав
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalAct" type="tns:TLegalAct" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Правоустанавливающий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="confirmPrivilege" type="tns:TConfirmPrivilege" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ, подтверждающий льготу</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="TSomeDocument">
        <xs:annotation>
            <xs:documentation>Набор документов прикладываемых к заявлению</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicSomeDocument">
                <xs:choice>
                    <xs:element name="otherDocument" type="tns:TOtherDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Прочий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="paymentDocument" type="tns:TPaymentDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Платежный документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="idDocument" type="tns:TIdDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorneyRestr">
                        <xs:annotation>
                            <xs:documentation>Доверенность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="mapPlanDocument" type="tns:TMapPlanDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Тех. план/межевой план/акт обследования/карта-план</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalAct" type="tns:TLegalActRestr">
                        <xs:annotation>
                            <xs:documentation>Правоустанавливающий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="confirmPrivilege" type="tns:TConfirmPrivilege">
                        <xs:annotation>
                            <xs:documentation>Документ, подтверждающий льготу</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TSomeRepresentativeDocument">
        <xs:annotation>
            <xs:documentation>Набор документов подтверждающих полномочия</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="document" type="tns:TStrictDocument">
                <xs:annotation>
                    <xs:documentation>Прочий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powerOfAttorney" type="tns:TStrictDocument">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalAct" type="tns:TLegalAct">
                <xs:annotation>
                    <xs:documentation>Правоустанавливающий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="TSomeInterdepDocument">
        <xs:annotation>
            <xs:documentation>Данные о документе по Межведу</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicSomeDocument">
                <xs:choice>
                    <xs:element name="document" type="tns:TDocument">
                        <xs:annotation>
                            <xs:documentation>Сведения о документе</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="interdepDocument" type="tns:TInterdepDocument">
                        <xs:annotation>
                            <xs:documentation>Документ сведений по МИВ (Межведомственному Информационному Взаимодействию):
                                -карта (план) объекта землеустройства
                                -описание местоположения границ соответствующих территорий или зон
                                -сведения для внесения в реестр ОН
                                -сведения для внесения в реестр Прав
                                -сведения для внесения в реестр границ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalAct" type="tns:TLegalAct">
                        <xs:annotation>
                            <xs:documentation>Реквизиты решений, правовых актов, иных документов, являющихся основанием для внесения сведений</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TDocument">
        <xs:annotation>
            <xs:documentation>Сведения о документе</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="2" name="documentTypes" type="tns:DocumentTypes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Информация по кодам документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="tns:SDocName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number" type="tns:SDocNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="issueDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="attachment" type="tns:TAttachment" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание приложенного файла</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="notes" type="commons:TNote">
                <xs:annotation>
                    <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="_id" type="xs:token" use="required">
            <xs:annotation>
                <xs:documentation>Идентификатор документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="DocumentTypes">
        <xs:choice maxOccurs="1">
            <xs:element name="documentTypeCode" type="dDocument:DDocument" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                <xs:annotation>
                    <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="TBasicStrictDocument">
        <xs:annotation>
            <xs:documentation>Документ, который может содержать признаки утверждения нотариусом, серию и др. юридически значимые параметры. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tns:TDocument">
                <xs:sequence>
                    <xs:element minOccurs="0" name="series" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Серия документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="issuer" type="tns:TIssuerInfo">
                        <xs:annotation>
                            <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="durationStart" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Срок действия C</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="durationStop" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Срок действия ПО</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="notaryInfo" type="tns:TNotaryInfo">
                        <xs:annotation>
                            <xs:documentation>Реквизиты о нотариальном заверении документа
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TStrictDocument">
        <xs:annotation>
            <xs:documentation>Документ, который может содержать признаки утверждения нотариусом, серию и др. юридически значимые параметры. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicStrictDocument">
                <xs:sequence>
                    <xs:sequence>
                        <xs:element maxOccurs="2" minOccurs="1" name="documentTypes"
                            type="tns:DocumentTypes">
                            <xs:annotation>
                                <xs:documentation>Информация по кодам документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="name" type="tns:SDocName">
                            <xs:annotation>
                                <xs:documentation>Наименование документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="number" type="tns:SDocNumber">
                            <xs:annotation>
                                <xs:documentation>Номер документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issueDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата выдачи</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="attachment" type="tns:TAttachment">
                            <xs:annotation>
                                <xs:documentation>Описание приложенного файла</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="notes"
                            type="commons:TNote">
                            <xs:annotation>
                                <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="series" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Серия документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issuer" type="tns:TIssuerInfo">
                            <xs:annotation>
                                <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStart" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия C</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="durationStop" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Срок действия ПО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="notaryInfo" type="tns:TNotaryInfo">
                            <xs:annotation>
                                <xs:documentation>Реквизиты о нотариальном заверении документа
                            </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TDocumentSeries">
        <xs:annotation>
            <xs:documentation>Документ, с необязательной серией</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TStrictDocument">
                <xs:sequence>
                    <xs:sequence>
                        <xs:element maxOccurs="2" minOccurs="1" name="documentTypes"
                            type="tns:DocumentTypes"/>
                        <xs:element minOccurs="0" name="name" type="tns:SDocName">
                            <xs:annotation>
                                <xs:documentation>Наименование документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="number" type="tns:SDocNumber">
                            <xs:annotation>
                                <xs:documentation>Номер документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issueDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата выдачи</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="attachment" type="tns:TAttachment">
                            <xs:annotation>
                                <xs:documentation>Описание приложенного файла</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="notes"
                            type="commons:TNote">
                            <xs:annotation>
                                <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="series" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Серия документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TBasicRequestDocument">
        <xs:annotation>
            <xs:documentation>Документ, для запроса копии документа. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicStrictDocument">
                <xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="name" type="tns:SDocName">
                            <xs:annotation>
                                <xs:documentation>Наименование документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="number" type="tns:SDocNumber">
                            <xs:annotation>
                                <xs:documentation>Номер документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="issueDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата выдачи</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="attachment" type="tns:TAttachment">
                            <xs:annotation>
                                <xs:documentation>Описание приложенного файла</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="notes"
                            type="commons:TNote">
                            <xs:annotation>
                                <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="series" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Серия документа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TRequestDocument">
        <xs:annotation>
            <xs:documentation>Документ, для запроса копии документа. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tns:TBasicRequestDocument">
                <xs:sequence>
                    <xs:element name="requestDocumentType" type="dReD:DRequestDocument">
                        <xs:annotation>
                            <xs:documentation>Тип запрашиваемого документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TDocumentWithNumber">
        <xs:annotation>
            <xs:documentation>StrictDocument с обязательным Номером Документа</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TStrictDocument">
                <xs:sequence>
                    <xs:element maxOccurs="2" name="documentTypes" type="tns:DocumentTypes">
                        <xs:annotation>
                            <xs:documentation>Информация по кодам документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="name" type="tns:SDocName" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Наименование документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="number" type="tns:SDocNumber">
                        <xs:annotation>
                            <xs:documentation>Номер документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="issueDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Дата выдачи</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="attachment" type="tns:TAttachment" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Описание приложенного файла</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="notes"
                        type="commons:TNote">
                        <xs:annotation>
                            <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="series" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Серия документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="issuer" type="tns:TIssuerInfo">
                        <xs:annotation>
                            <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="durationStart" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Срок действия C</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="durationStop" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Срок действия ПО</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="notaryInfo" type="tns:TNotaryInfo">
                        <xs:annotation>
                            <xs:documentation>Реквизиты о нотариальном заверении документа
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TPaymentDocument">
        <xs:annotation>
            <xs:documentation>Платёжный документ</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tns:TStrictDocument">
                <xs:sequence>
                    <xs:element name="supplierBillId" type="tns:SDocNumber" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Универсальный идентификатор начисления (УИН)</xs:documentation>
                        </xs:annotation>
