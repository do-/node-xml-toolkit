<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
           targetNamespace="http://rosreestr.ru/services/v0.18/KUVD"
           xmlns:tns="http://rosreestr.ru/services/v0.18/KUVD"
           xmlns:com="http://rosreestr.ru/services/v0.1/commons/Commons"
           xmlns:pers="http://rosreestr.ru/services/v0.1/commons/Subjects"
           xmlns:stCom="http://rosreestr.ru/services/v0.1/TStatementCommons"
           xmlns:dReM="http://rosreestr.ru/services/v0.1/commons/directories/receivingMethod"
           vc:minVersion="1.1" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
           xmlns:docs="http://rosreestr.ru/services/v0.1/commons/Documents">

    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/Commons"
               schemaLocation="Commons.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/Subjects"
               schemaLocation="TSubject.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/TStatementCommons"
               schemaLocation="TStatementCommons.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/Documents"
               schemaLocation="TDocument.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commons/directories/receivingMethod"
               schemaLocation="Dictionary/DReceivingMethod.xsd"/>
    <xs:notation name="KUVDBook" public="">
        <xs:annotation>
            <xs:documentation>Книга Учета Входящих Документов</xs:documentation>
        </xs:annotation>
    </xs:notation>
    <xs:complexType name="TKUVDBook">
        <xs:annotation>
            <xs:documentation>Книга учета входящих документов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование книги учета документов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number">
                <xs:annotation>
                    <xs:documentation>Порядковый номер книги учета документов, ведущийся органом регистрации прав</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="\d{3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="year">
                <xs:annotation>
                    <xs:documentation>Год, в котором осуществляется ведение книги учета документов</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="\d{4}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="regRightAuthorityName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование органа регистрации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="regRightAuthorityCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код органа регистрации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="tns:DBookStatus">
                <xs:annotation>
                    <xs:documentation>Статус КУВД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="finalCounter" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Количество записей в КУВД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateOpen" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата и время открытия КУВД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="dateClose" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата и время закрытия КУВД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="inheritedParams" type="tns:TKUVDBookInheritedParams">
                <xs:annotation>
                    <xs:documentation>Параметры КУВД унаследованных систем</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="note" type="tns:TKUVDNote">
                <xs:annotation>
                    <xs:documentation>Комментарии к книге</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bookNmbrUis" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер КУВД в УИС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:notation name="KUVDBookHelpers" public=""/>
    <xs:complexType name="TKUVDBookInheritedParams">
        <xs:annotation>
            <xs:documentation>Параметры КУВД унаследованных систем</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="expertStrt" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Кем открыта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="expertStop" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Кем закрыта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="recordStrt" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер первой записи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="recordStop" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер последней записи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="forPackets" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Для пакетов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="statusHed" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Статус ХЭД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ownerName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Тер. принадлежность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ownerCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Принадлежность: Номер</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="updDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Последнее обновление: Дата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="updExpert" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Последнее обновление: Оператор</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="updOwner" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Последнее обновление: Подразделение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="insDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Внесение записи: Дата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="insExpert" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Внесение записи: Оператор</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="sourceRec" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Источник поступления записей в книге</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="commentary" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Комментарий</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sourceType" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Тип УИС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:notation name="KUVDRecord" public=""/>
    <xs:complexType name="TBookRecord">
        <xs:sequence>
            <xs:element name="bookNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер книги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Порядковый номер записи в книги, начиная с 1 с шагом 1</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="receiveDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата приема заявления с точностью до минуты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="moscowReceiveDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата приема заявления с точностью до минуты по времени Москвы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="author" type="tns:TAuthorSubject">
                <xs:annotation>
                    <xs:documentation>Данные автора изменений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="regRightAuthorityCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код подразделения по справочнику ЕСТО </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="regRightAuthorityName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование подразделения по справочнику ЕСТО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="notes" type="tns:TKUVDNote">
                <xs:annotation>
                    <xs:documentation>Дополнительные сведения</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="_id" type="xs:token" use="required"/>
    </xs:complexType>
    <xs:complexType name="TCloseBookRecord">
        <xs:complexContent>
            <xs:extension base="tns:TBookRecord">
                <xs:sequence>
                    <xs:element name="closeDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Дата закрытия книги</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TOpenBookRecord">
        <xs:complexContent>
            <xs:extension base="tns:TBookRecord">
                <xs:sequence>
                    <xs:element name="openDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Дата открытия книги</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TKUVDRecord">
        <xs:annotation>
            <xs:documentation>Запись КУВД</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tns:TBookRecord">
                <xs:sequence>
                    <xs:element name="actionCode" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Код УРД (справоник ЕС УНСИ)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="action" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Цель запроса</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="receivingMethodCode"
                                type="dReM:DReceivingMethod">
                        <xs:annotation>
                            <xs:documentation>Способ получения заявления</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="details" maxOccurs="unbounded" type="tns:TKUVDStatementDetails"/>
                    <xs:element default="false" name="hasReceipt" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>Признак выдачи расписки</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="resultInfo" minOccurs="0" type="tns:TKUVDRecordResultInfo">
                        <xs:annotation>
                            <xs:documentation>Результат</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="editedData"
                                type="tns:TKUVDRecordEditData">
                        <xs:annotation>
                            <xs:documentation>Записи истории изменения записи КУВД</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="inheritedParams"
                                type="tns:TKUVDRecordInheritedParams">
                        <xs:annotation>
                            <xs:documentation>Атрибуты записи КУВД наследуемых систем</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:notation name="KUVDRecordDocuments" public=""/>
    <xs:complexType name="TKUVDDocument">
        <xs:annotation>
            <xs:documentation>Документ из записи КУВД</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="documentTypes" type="tns:TKUVDDocTypes">
                <xs:annotation>
                    <xs:documentation>Типы документа по справочникам ЕС УНСИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="name" type="docs:SDocName"/>
            <xs:element minOccurs="0" name="number" type="docs:SDocNumber">
                <xs:annotation>
                    <xs:documentation>Номер документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="issueDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="attachments" type="docs:TAttachment" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Описание приложенного(-ых) файла(-ов)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="notes" type="com:TNote">
                <xs:annotation>
                    <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="series" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Серия документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="issuer" type="docs:TIssuerInfo">
                <xs:annotation>
                    <xs:documentation>Организация, выдавшая документ, или автор документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="durationStart" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Срок действия C</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="durationStop" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Срок действия ПО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="notaryInfo" type="docs:TNotaryInfo">
                <xs:annotation>
                    <xs:documentation>Реквизиты о нотариальном заверении документа
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="inheritedParams" type="tns:TKUVDDocInheritedParams">
                <xs:annotation>
                    <xs:documentation>Атрибуты Документа из записи КУВД наследуемых систем</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TKUVDDocTypes">
        <xs:annotation>
            <xs:documentation>Тип документа по справочникам ЕС УНСИ с уточнением наименования</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="docs:DocumentTypes">
                <xs:sequence>
                    <xs:element name="documentTypeName" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TKUVDStatementDetails">
        <xs:annotation>
            <xs:documentation>Детали заявления</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="documentsQuantity" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Общее количество принятых документов (включая заявление)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pageQuantity" type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Количество листов в принятых документах</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="subjects" type="pers:TBasicStatementSubject" maxOccurs="unbounded"/>
            <xs:element name="objects" type="stCom:TStatementObjects"/>
            <xs:element name="documents" type="tns:TKUVDSomeDocument" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Набор документов прикладываемых к Записи КУВД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="subjectsInheritedParams"
                        type="tns:TKUVDRecordSubjectsInheritedParams">
                <xs:annotation>
                    <xs:documentation>Данные по субъектам из унаследованных систем</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="representativesIneritedParams"
                        type="tns:TKUVDRecordRepresentativeInheritedParams"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="objectsInheritedParams"
                        type="tns:TKUVDRecordObjectInheritedParams"/>
        </xs:sequence>
        <xs:attribute name="statementID" type="xs:token" use="required">
            <xs:annotation>
                <xs:documentation>Номер заявления</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TKUVDSomeDocument">
        <xs:annotation>
            <xs:documentation>Набор документов прикладываемых к Записи КУВД</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="document" type="tns:TKUVDDocument">
                <xs:annotation>
                    <xs:documentation>Принятые документы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="requestedDocument" type="tns:TKUVDDocument">
                <xs:annotation>
                    <xs:documentation>Запрашиваемые документы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="outDocument" type="tns:TKUVDDocument">
                <xs:annotation>
                    <xs:documentation>Исходящие документы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentDocument" type="tns:TKUVDPaymentDocument">
                <xs:annotation>
                    <xs:documentation>Оплата</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
        <xs:attribute name="_id" type="xs:token" use="required">
            <xs:annotation>
                <xs:documentation>Номер заявления</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TKUVDPaymentDocument">
        <xs:annotation>
            <xs:documentation>Оплата</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="docs:TPaymentDocument">
                <xs:sequence>
                    <xs:element minOccurs="0" name="inheritedParams"
                                type="tns:TKUVDPaymentDocumentInheritedParams"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TKUVDPaymentDocumentInheritedParams">
        <xs:sequence>
            <xs:element name="paymentInvoice" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>№ счет-фактуры</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentBack" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сумма возврата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentPurpose" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Назначение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentOkato" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код ОКАТО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ks" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>К\С</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="payerType" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Тип плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentBase" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Основание платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentConfirm" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Подтверждение платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="recievePaymentDate" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата поступления сведений об оплате гос.пошлины</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="needConfirmPayment" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Необходимость подтверждения уплаты гос. Пошлины</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="representation" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код представительства, к которому относится запись</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentDirection" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Направление платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentCode" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentStatus" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Состояние оплаты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="comment" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Примечание</xs:documentation>
                </xs:annotation>
            </xs:element>
